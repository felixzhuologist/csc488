{
  write "open 0", newline
  repeat {
    write "open 1", newline
    repeat {
      write "open 2", newline
        repeat {
          write "open 3", newline
          while true do {
            exit 3
            write "inside 3", newline
          }
          write "close 3", newline
        } until true
        write "close 2", newline
    } until true
    write "close 1", newline
  } until true
  write "close 0", newline
}